{
  "kind": "collectionType",
  "collectionName": "mid_ads",
  "info": {
    "singularName": "mid-ad",
    "pluralName": "mid-ads",
    "displayName": "MidAd"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "link": {
      "type": "string",
      "required": true
    },
    "isNewTab": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "priority": {
      "type": "integer",
      "required": true,
      "default": 1,
      "min": 1
    },
    "position": {
      "type": "enumeration",
      "required": true,
      "default": "all",
      "enum": [
        "all",
        "home",
        "games"
      ]
    }
  }
}
